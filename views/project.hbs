                        {{#each projects}}
                         <div class="my-project">
                    <div class="card shadow-lg mb-4" style="width: 26rem;">
                        <img class="card-img-top" name= 'upload' src={{this.upload}} alt="Card image cap">
                        <div class="card-body">
                     
                            <h5 class="card-title" ; style=" margin-left: 10px; text-decoration: none;"><a href="/detail-project/{{this.id}}" style="color:black">{{this.projectName}} - {{this.name}}</a></h5>
                            <small style="color: rgb(134, 130, 130); font-style : 'italic' ;margin-left: 10px; ">Di posting pada :{{this.time}}</small><br>
                            <small style="color: rgb(134, 130, 130); margin-left: 10px; ">durasi :{{this.duration}}</small>
                            <br>
                            <p class="card-text ">{{this.description}}
                            </p>
                            {{#each technologies}}
                            <div class="logo ">
                                <img src="/public/img/{{this}}">
                            </div>
                            {{/each}}
